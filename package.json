{
  "name": "typesplainer",
  "displayName": "typesplainer",
  "description": "Explanation for python typehints right in vscode",
  "version": "0.0.1",
  "author": "Wasi Master <arianmollik323@gmail.com> (https://wasi-master.github.io)",
  "publisher": "WasiMaster",
  "icon": "logo.png",
  "homepage": "https://wasi-master.github.io/typesplainer-vscode",
  "bugs": {
    "url": "https://github.com/wasi-master/typesplainer-vscode/issues",
    "email": "arianmollik323@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wasi-master/typesplainer-vscode"
  },
  "keywords": [
    "python",
    "typehint",
    "type hint",
    "typehint explainer",
    "typehint explain",
    "type hint explainer",
    "type hint explain",
    "explain typehints",
    "explain type hints",
    "python typehint",
    "python type hint",
    "python typehint explainer",
    "python typehint explain",
    "python type hint explainer",
    "python type hint explain",
    "python explain typehints",
    "python explain type hints"
  ],
  "engines": {
    "vscode": "^1.31.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onCommand:typesplainer.explainType"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "typesplainer.openWebsite",
        "title": "Typesplainer: Open Website"
      }
    ],
    "configuration": [
      {
        "title": "Typesplainer",
        "properties": {
          "typesplainer.format": {
            "type": "string",
            "default": "python",
            "markdownDescription": "Specify a custom command for python"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "1.31",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "compare-versions": "^4.1.3",
    "lru-cache-js-map": "^1.0.7",
    "node-version-compare": "^1.0.3"
  }
}
